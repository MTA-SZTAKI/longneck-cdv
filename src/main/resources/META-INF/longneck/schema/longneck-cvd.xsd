<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
           xmlns:tns="urn:hu.sztaki.ilab.longneck:1.0"
           targetNamespace="urn:hu.sztaki.ilab.longneck:1.0" 
           elementFormDefault="qualified">

  <xs:complexType name="cvdType">
    <xs:complexContent>
      <xs:extension base="tns:atomicConstraintType">
        <xs:attributeGroup ref="tns:atomicConstraintAttrGroup"/>
        <xs:attribute name="coefficients" type="xs:string" use="required"/>
        <xs:attribute name="mod" type="xs:string" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:element name="cvd" type="tns:cvdType" substitutionGroup="tns:abstract-constraint"/>

</xs:schema>
